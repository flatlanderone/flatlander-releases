<ramblingman>

<conditional>
<if cond="mod_loaded('0-SCore_sphereii') and mod_version('0-SCore_sphereii') >= version(2,4,11,1222)">


<!-- *** Edit intro_buried_supplies. -->
<!-- Remove intro_buried_supplies from each trader's quest pool. intro_buried_supplies is autocompleted on first spawn. -->
<remove xpath="/quests/quest_list[@id='trader_rekt_quests' or @id='trader_jen_quests' or @id='trader_bob_quests' or@id='trader_hugh_quests' or @id='trader_joel_quests']/quest[@id='intro_buried_supplies']"/> 


<!-- *** Edit quest_whiteRiverCitizen1. -->

<!-- Remove Goto objective as it didn't fire inside the trader's compound. -->
<remove xpath="/quests/quest[@id='quest_whiteRiverCitizen1']/objective[@type='Goto']" />

<!-- Remove stone shovel reward -->

<remove xpath="/quests/quest[@id='quest_whiteRiverCitizen1']/reward[@id='meleeToolShovelT0StoneShovel']" />

<!-- Remove the land claim block challenge reminder, as it prevents the first regular quest from showing. -->
<remove xpath="/quests/quest[@id='quest_whiteRiverCitizen1']/action[@type='TrackChallenge']" />

<!-- Remove vanilla xp award.  -->
<remove xpath="/quests/quest[@id='quest_whiteRiverCitizen1']/reward[@type='Exp' and @value='500']" />

<!-- Merge rewards from quest_whiteRiverCitizen1 and intro_buried_supplies -->
<append xpath="/quests/quest[@id='quest_whiteRiverCitizen1']">

	<property name="reward_choices_count" value="5"/>
	
	<!-- Xp from WRC (600)  and IBS (500) -->
    <reward type="Exp" value="1000"/>
	<!-- Items from IBS -->
	<reward type="Item" id="questRewardPipePistolBundle" ischosen="true" value="1"/>
	<reward type="Item" id="questRewardPipeShotgunBundle" ischosen="true" value="1"/>
	<reward type="Item" id="questRewardPipeRifleBundle" ischosen="true" value="1"/>
	<reward type="Item" id="questRewardPipeMachineGunBundle" ischosen="true" value="1"/>
	<reward type="Item" id="questRewardPipeBatonBundle" ischosen="true" value="1"/>
</append>

<append xpath="/quests">

<!-- Use as template for QS <other trader>  -->

<!-- *** Extreme Quick Start - Trader Rekt -->

<quest id="q_goToPOI_trader_rekt_helipadQS">
    <property name="group_name_key" value="q_goToPOI_teleport_to_traders"/>
    <property name="name_key" value="q_goToPOI_trader_rekt_helipadQS"/>
    <property name="subtitle_key" value="q_goToPOI_trader_rekt_helipadQS_subtitle"/>
